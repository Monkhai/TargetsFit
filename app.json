{
  "expo": {
    "name": "TargetsFit",
    "slug": "TargetsFit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yohaiwiener.TargetsFit",
      "buildNumber": "8",
      "splash": {
        "dark": {
          "backgroundColor": "#000",
          "image": "./assets/images/dark-splash.png"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 2,
      "splash": {
        "dark": {
          "image": "./assets/images/dark-splash.png",
          "backgroundColor": "#000"
        }
      },
      "package": "com.yohaiwiener.TargetsFit"
    },
    "plugins": ["expo-router", ["expo-av", { "microphonePremission": "Allow $(PRODUCT_NAME) to access your microphone." }]],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f4f3d740-4893-464b-9fa1-4d85084e2ff0"
      }
    },
    "owner": "yohaiwiener"
  }
}
